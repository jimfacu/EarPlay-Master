<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/width130"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/margin15"
    android:layout_marginVertical="@dimen/marginVertica10"
    android:background="@color/Negro"
    android:orientation="vertical">


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageView_ArtistRank"
        android:layout_width="@dimen/width130"
        android:layout_height="130dp"
        android:src="@drawable/xd"
        app:civ_border_color="#FFFFFF"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toTopOf="@+id/textView_ArtistName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView_ArtistName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="BADBUNY"
        android:textColor="@color/Blanco"
        android:textSize="@dimen/textSize20"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/circleImageView_ArtistRank" />
</androidx.constraintlayout.widget.ConstraintLayout>